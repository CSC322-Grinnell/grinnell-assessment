<% provide(:title, "Institutional Goals") %>
<h1>Institutional Goals</h1>

<table>
  <thead>
    <tr>
      <th colspan="2">Goal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instgoals.each do |instgoal| %>
      <tr>
        <td><%= instgoal.goalNum %></td>
        <td><%= instgoal.goalText %></td>
        <td><%= link_to 'Show', instgoal %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
